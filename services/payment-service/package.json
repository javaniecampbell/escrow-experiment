{
  "name": "payment-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dist": "node -r ./utils/tracing.js ./dist/bin/www",
    "dev": "cross-env DEBUG=payment-service:* & ts-node-dev --respawn --transpile-only --ignore-watch node_modules ./src/bin/www",
    "postinstall": "npx prisma generate",
    "develop": "SET DEBUG=payment-service:* & npm start",
    "dev:win32": "SET DEBUG=payment-service:* & ts-node-dev --respawn --transpile-only --ignore-watch node_modules ./src/bin/www",
    "build": "tsc",
    "prebuild": "rimraf dist && mkdir dist && cpy src/views/* dist/views --cwd=./ && cpy src/public/* dist/public --cwd=./ && cpy src/bin/* dist/bin --cwd=./"
  },
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/storage-blob": "^12.17.0",
    "@ddaw/healthcheck-sdk": "*",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.45.0",
    "@opentelemetry/exporter-jaeger": "^1.24.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.51.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.51.1",
    "@opentelemetry/exporter-prometheus": "^0.51.0",
    "@opentelemetry/instrumentation-http": "^0.51.0",
    "@opentelemetry/sdk-metrics": "^1.24.1",
    "@opentelemetry/sdk-node": "^0.51.0",
    "@opentelemetry/sdk-trace-base": "^1.24.0",
    "@opentelemetry/sdk-trace-node": "^1.24.0",
    "@opentelemetry/semantic-conventions": "^1.24.1",
    "@opentelemetry/winston-transport": "^0.3.0",
    "@prisma/client": "^5.14.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "morgan": "~1.9.1",
    "stripe": "^14.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "http-errors": "~1.6.3",
    "nanoid": "^4.0.1",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "prisma": "^5.14.0",
    "prisma-dbml-generator": "^0.12.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.13.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
